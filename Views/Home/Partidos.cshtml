@{
    ViewData["Title"] = "Partidos";
}
<button class="d-none" id="btn-imgusuario" onclick="MostrarFotoUsuario('@ViewBag.Usuario.Foto', '@ViewBag.Usuario.Nombre', '@ViewBag.Usuario.idUsuario')"></button>

<div class="text-center">
    <div class="row">
        <h1 class="blanco">Partidos de la fecha</h1>

            
                @foreach (Partido item in ViewBag.ListarPartidos)
                {
                    <div class="col-md-2 col-sm-10">
                    <table class="table content-center table-dark ">
                    <thead>
                        <tr>
                            <th scope="col-1" class="th-equipo">River</th>
                            <th scope="col-1" class="th-equipo">0</th>

                        </tr>
                    </thead>
                    <tbody>
                        <tr class="tr-equipo">
                            <th scope="col-1" class="th-equipo">Boca</th>
                            <th scope="col-1" class="th-equipo">0</th>
                        </tr>
                    </tbody>
                </table>
                </div>
                }



    </div>





</div>
